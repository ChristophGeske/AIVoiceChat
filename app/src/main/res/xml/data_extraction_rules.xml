<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <!-- Exclude API keys/preferences from cloud backup and device-to-device transfer on API 31+ -->
    <cloud-backup>
        <exclude domain="sharedpref" path="ai_prefs.xml"/>
    </cloud-backup>
    <device-transfer>
        <exclude domain="sharedpref" path="ai_prefs.xml"/>
    </device-transfer>
</data-extraction-rules>